public class Product {
private int id;
private String SKU; //stock keeping unit (a unique code for each product)
private String description;
private String category;
private float price;
Product(int id, String SKU, String description, String category, float price){
  this.id = id
  this.SKU = SKU;
  this.description = description;
  this.category = category;
  this.price = price;
  
}
  void list() {
  System.out.println("Welcome, What Would We Be Delivering Today?!");
  }
  
}
